<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pomo.toasterfx.demo.controller.ProgressToastController">
   <children>
      <Label fx:id="title" text="Label" />
      <ProgressBar fx:id="progress" maxWidth="1.7976931348623157E308" progress="0.0" />
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#onAction" text="Button" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="15.0" />
   </padding>
</VBox>
